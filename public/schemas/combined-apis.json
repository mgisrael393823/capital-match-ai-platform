{
  "openapi": "3.1.0",
  "info": {
    "title": "Capital Match AI Platform APIs",
    "description": "Combined APIs for zoning lookup, investor matching, market comparison, and entitlement tracking, leveraging municipal open data sources",
    "version": "1.0.0",
    "x-data-sources": {
      "city-portals": [
        {
          "city": "Chicago",
          "portal": "data.cityofchicago.org",
          "datasets": ["Parcels", "zoning", "permits", "land use"],
          "api-endpoint": "https://data.cityofchicago.org/resource/nifi-zqag.json"
        },
        {
          "city": "Denver",
          "portal": "opendata.denvergov.org",
          "datasets": ["Rezoning cases", "permits", "zoning overlays"],
          "api-endpoint": "https://www.denvergov.org/api/data/open-data"
        },
        {
          "city": "Nashville",
          "portal": "data.nashville.gov",
          "datasets": ["Land use", "council actions", "permits"],
          "api-endpoint": "https://data.nashville.gov/resource/xakp-ess3.json"
        },
        {
          "city": "Charlotte",
          "portal": "data.charlottenc.gov",
          "datasets": ["Zoning", "TOD", "boundaries"],
          "api-endpoint": "https://data.charlottenc.gov/resource/dqf5-yhfm.json"
        },
        {
          "city": "Raleigh",
          "portal": "data.raleighnc.gov",
          "datasets": ["Entitlements", "zoning layers"],
          "api-endpoint": "https://data.raleighnc.gov/resource/v8b7-rwkf.json"
        }
      ]
    }
  },
  "servers": [
    {
      "url": "https://capital-match-ai-platform.vercel.app/api",
      "description": "Production server"
    },
    {
      "url": "/api",
      "description": "Local API server"
    }
  ],
  "paths": {
    "/zoning-filter": {
      "post": {
        "summary": "Filter parcels by zoning criteria",
        "operationId": "filterParcelsByZoning",
        "tags": [
          "Parcel Filtering"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "city",
                  "filters"
                ],
                "properties": {
                  "city": {
                    "type": "string",
                    "description": "The city to search parcels",
                    "example": "Charlotte",
                    "enum": [
                      "Charlotte",
                      "Raleigh"
                    ]
                  },
                  "filters": {
                    "type": "object",
                    "properties": {
                      "zoning_districts": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        },
                        "example": ["MUDD", "TOD-NC"]
                      },
                      "min_lot_size": {
                        "type": "number",
                        "description": "Minimum lot size in square feet",
                        "example": 10000
                      },
                      "max_lot_size": {
                        "type": "number",
                        "description": "Maximum lot size in square feet",
                        "example": 50000
                      },
                      "overlays": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        },
                        "example": ["TOD-1", "Historic District"]
                      },
                      "opportunity_zone": {
                        "type": "boolean",
                        "example": true
                      },
                      "proximity_to_transit": {
                        "type": "number",
                        "description": "Maximum distance to transit in miles",
                        "example": 0.5
                      }
                    }
                  },
                  "boundary": {
                    "type": "object",
                    "description": "Geographic boundary to limit search",
                    "properties": {
                      "center": {
                        "type": "object",
                        "properties": {
                          "lat": {
                            "type": "number",
                            "example": 35.2271
                          },
                          "lng": {
                            "type": "number",
                            "example": -80.8431
                          }
                        }
                      },
                      "radius": {
                        "type": "number",
                        "description": "Search radius in miles",
                        "example": 1.5
                      },
                      "neighborhood": {
                        "type": "string",
                        "description": "Named neighborhood as boundary",
                        "example": "Uptown"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successfully filtered parcels",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "city": {
                      "type": "string",
                      "example": "Charlotte"
                    },
                    "parcels_found": {
                      "type": "number",
                      "example": 23
                    },
                    "parcels": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "address": {
                            "type": "string",
                            "example": "401 N Tryon St, Charlotte, NC"
                          },
                          "parcel_id": {
                            "type": "string",
                            "example": "08117716"
                          },
                          "zoning": {
                            "type": "string",
                            "example": "UMUD"
                          },
                          "overlays": {
                            "type": "array",
                            "items": {
                              "type": "string"
                            },
                            "example": ["TOD-CC"]
                          },
                          "lot_size": {
                            "type": "number",
                            "example": 22500
                          },
                          "opportunity_zone": {
                            "type": "boolean",
                            "example": true
                          },
                          "distance_to_transit": {
                            "type": "number",
                            "example": 0.2
                          }
                        }
                      }
                    },
                    "filter_applied": {
                      "type": "object",
                      "description": "Echo of the filters that were applied"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request - missing required parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "City parameter is required"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    
    "/tod-overlay": {
      "get": {
        "summary": "Get Transit-Oriented Development overlay information",
        "operationId": "getTODOverlay",
        "tags": [
          "Overlays"
        ],
        "parameters": [
          {
            "name": "address",
            "in": "query",
            "description": "The property address to look up TOD information",
            "required": true,
            "schema": {
              "type": "string",
              "example": "500 S College St, Charlotte, NC"
            }
          },
          {
            "name": "city",
            "in": "query",
            "description": "The city to look up TOD information",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "Charlotte",
                "Raleigh"
              ],
              "example": "Charlotte"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with TOD overlay information",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "address": {
                      "type": "string",
                      "example": "500 S College St, Charlotte, NC"
                    },
                    "city": {
                      "type": "string",
                      "example": "Charlotte"
                    },
                    "tod_overlay": {
                      "type": "object",
                      "properties": {
                        "district": {
                          "type": "string",
                          "example": "TOD-UC"
                        },
                        "description": {
                          "type": "string",
                          "example": "Transit Oriented Development - Urban Center"
                        },
                        "max_height": {
                          "type": "number",
                          "example": 120
                        },
                        "max_density": {
                          "type": "number",
                          "example": 250
                        },
                        "parking_reduction": {
                          "type": "number",
                          "example": 50
                        },
                        "closest_station": {
                          "type": "string",
                          "example": "Stonewall Station"
                        },
                        "distance_to_station": {
                          "type": "number",
                          "example": 0.3
                        }
                      }
                    },
                    "opportunity_zone": {
                      "type": "boolean",
                      "example": true
                    },
                    "additional_overlays": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "example": ["UDIO", "HDO"]
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request - missing required parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Address and city parameters are required"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    
    "/upzoned-scanner": {
      "get": {
        "summary": "Scan for recently upzoned parcels in Nashville",
        "operationId": "scanUpzonedParcels",
        "tags": [
          "Zoning Changes"
        ],
        "parameters": [
          {
            "name": "timeframe",
            "in": "query",
            "description": "Timeframe to scan for upzoned parcels",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "30days",
                "90days",
                "180days",
                "1year"
              ],
              "default": "90days",
              "example": "90days"
            }
          },
          {
            "name": "district",
            "in": "query",
            "description": "Limit scan to specific council district",
            "required": false,
            "schema": {
              "type": "string",
              "example": "District 19"
            }
          },
          {
            "name": "minimum_increase",
            "in": "query",
            "description": "Minimum increase in development potential",
            "required": false,
            "schema": {
              "type": "number",
              "example": 50
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with upzoned parcels",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "timeframe": {
                      "type": "string",
                      "example": "90days"
                    },
                    "total_upzoned_parcels": {
                      "type": "number",
                      "example": 47
                    },
                    "upzoned_parcels": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "address": {
                            "type": "string",
                            "example": "1234 5th Ave N, Nashville, TN"
                          },
                          "parcel_id": {
                            "type": "string",
                            "example": "10505001300"
                          },
                          "previous_zoning": {
                            "type": "string",
                            "example": "RS5"
                          },
                          "new_zoning": {
                            "type": "string",
                            "example": "RM20-A"
                          },
                          "rezone_date": {
                            "type": "string",
                            "format": "date",
                            "example": "2023-07-12"
                          },
                          "council_case_number": {
                            "type": "string",
                            "example": "BL2023-1584"
                          },
                          "density_increase": {
                            "type": "number",
                            "example": 300
                          },
                          "height_increase": {
                            "type": "number",
                            "example": 50
                          },
                          "council_district": {
                            "type": "string",
                            "example": "District 19"
                          }
                        }
                      }
                    },
                    "market_trends": {
                      "type": "object",
                      "properties": {
                        "most_upzoned_districts": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          },
                          "example": ["District 19", "District 5", "District 21"]
                        },
                        "most_common_upzoning": {
                          "type": "string",
                          "example": "RS5 to RM20-A"
                        },
                        "average_density_increase": {
                          "type": "number",
                          "example": 175
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request - invalid parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Invalid timeframe parameter"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    
    "/zoning": {
      "get": {
        "summary": "Retrieve parcel and zoning information for a given address",
        "operationId": "getZoningInfo",
        "tags": [
          "Zoning"
        ],
        "parameters": [
          {
            "name": "address",
            "in": "query",
            "description": "The property address to look up (e.g., Chicago, Denver, Charlotte, Raleigh, Nashville)",
            "required": true,
            "schema": {
              "type": "string",
              "example": "Willis Tower, Chicago"
            }
          },
          {
            "name": "city",
            "in": "query",
            "description": "The city to look up zoning information",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "Chicago",
                "Denver",
                "Charlotte",
                "Raleigh",
                "Nashville"
              ],
              "example": "Chicago"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with parcel and zoning data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "coordinates": {
                      "type": "object",
                      "properties": {
                        "lat": {
                          "type": "number",
                          "example": 41.8781
                        },
                        "lng": {
                          "type": "number",
                          "example": -87.6298
                        }
                      }
                    },
                    "zoning": {
                      "type": "object",
                      "properties": {
                        "zoning_classification": {
                          "type": "string",
                          "example": "B3-5"
                        },
                        "description": {
                          "type": "string",
                          "example": "Community Shopping District"
                        },
                        "overlay_districts": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          },
                          "example": ["TOD-2", "Opportunity Zone"]
                        }
                      }
                    },
                    "parcel": {
                      "type": "object",
                      "properties": {
                        "pin": {
                          "type": "string",
                          "example": "17091230120000"
                        },
                        "property_class": {
                          "type": "string",
                          "example": "5-99"
                        },
                        "township_name": {
                          "type": "string",
                          "example": "CHICAGO"
                        },
                        "square_footage": {
                          "type": "number",
                          "example": 5000
                        }
                      }
                    },
                    "address_queried": {
                      "type": "string",
                      "example": "Willis Tower, Chicago"
                    },
                    "city": {
                      "type": "string",
                      "example": "Chicago"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request - missing required parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Address parameter is required"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/investor-match": {
      "post": {
        "summary": "Find investors that match a given real estate deal",
        "operationId": "matchInvestors",
        "tags": [
          "Investor Matching"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "assetType",
                  "market",
                  "investmentAmount",
                  "expectedReturn",
                  "riskProfile"
                ],
                "properties": {
                  "assetType": {
                    "type": "string",
                    "description": "The type of real estate asset",
                    "example": "multifamily",
                    "enum": [
                      "multifamily",
                      "office",
                      "industrial",
                      "retail",
                      "hotel",
                      "student housing",
                      "mixed-use"
                    ]
                  },
                  "market": {
                    "type": "string",
                    "description": "The geographic market/city where the property is located",
                    "example": "Chicago"
                  },
                  "investmentAmount": {
                    "type": "number",
                    "description": "The amount of investment required in USD",
                    "example": 5000000
                  },
                  "expectedReturn": {
                    "type": "number",
                    "description": "The expected annual return percentage",
                    "example": 12
                  },
                  "riskProfile": {
                    "type": "string",
                    "description": "The risk level of the investment",
                    "example": "moderate",
                    "enum": [
                      "low",
                      "moderate",
                      "high"
                    ]
                  },
                  "projectName": {
                    "type": "string",
                    "description": "Optional name of the project/property",
                    "example": "Lakeview Apartments"
                  },
                  "description": {
                    "type": "string",
                    "description": "Optional brief description of the investment opportunity",
                    "example": "256-unit apartment complex in Lakeview neighborhood"
                  },
                  "timeline": {
                    "type": "string",
                    "description": "Optional timeline for the investment",
                    "example": "5 years"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successfully matched investors with the deal",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "deal": {
                      "type": "object",
                      "properties": {
                        "assetType": {
                          "type": "string",
                          "example": "multifamily"
                        },
                        "market": {
                          "type": "string",
                          "example": "Chicago"
                        },
                        "investmentAmount": {
                          "type": "number",
                          "example": 5000000
                        },
                        "expectedReturn": {
                          "type": "number",
                          "example": 12
                        },
                        "riskProfile": {
                          "type": "string",
                          "example": "moderate"
                        }
                      }
                    },
                    "matches": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "investorId": {
                            "type": "string",
                            "example": "INV001"
                          },
                          "investorName": {
                            "type": "string",
                            "example": "Blackstone Real Estate"
                          },
                          "matchScore": {
                            "type": "number",
                            "example": 85
                          },
                          "matchDetails": {
                            "type": "object",
                            "properties": {
                              "assetTypeMatch": {
                                "type": "boolean",
                                "example": true
                              },
                              "marketMatch": {
                                "type": "boolean",
                                "example": true
                              },
                              "investmentSizeMatch": {
                                "type": "boolean",
                                "example": true
                              },
                              "returnExpectationMatch": {
                                "type": "boolean",
                                "example": false
                              },
                              "riskProfileMatch": {
                                "type": "boolean",
                                "example": true
                              }
                            }
                          }
                        }
                      }
                    },
                    "totalMatches": {
                      "type": "number",
                      "example": 3
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request - missing required parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Missing required field: assetType"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/entitlement-tracking": {
      "get": {
        "summary": "Track entitlement and permit status for a given property",
        "operationId": "trackEntitlements",
        "tags": [
          "Entitlements"
        ],
        "parameters": [
          {
            "name": "address",
            "in": "query",
            "description": "The property address to look up entitlements for",
            "required": true,
            "schema": {
              "type": "string",
              "example": "1700 Lincoln St, Denver, CO"
            }
          },
          {
            "name": "city",
            "in": "query",
            "description": "The city to look up entitlement information",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "Denver",
                "Chicago",
                "Charlotte",
                "Raleigh",
                "Nashville",
                "New York",
                "Los Angeles",
                "Miami",
                "Dallas"
              ],
              "example": "Denver"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with entitlement tracking data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "city": {
                      "type": "string",
                      "example": "denver"
                    },
                    "address": {
                      "type": "string",
                      "example": "1700 Lincoln St, Denver, CO"
                    },
                    "permits": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "type": {
                            "type": "string",
                            "example": "Rezoning"
                          },
                          "status": {
                            "type": "string",
                            "example": "Approved"
                          },
                          "reference_number": {
                            "type": "string",
                            "example": "Z-2023-10456"
                          },
                          "filing_date": {
                            "type": "string",
                            "example": "2023-05-15"
                          },
                          "last_update": {
                            "type": "string",
                            "example": "2023-09-22"
                          },
                          "description": {
                            "type": "string",
                            "example": "Rezoning from C-MX-5 to C-MX-8"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request - missing required parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Both city and address are required."
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    
